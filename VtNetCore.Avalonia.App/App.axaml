<Application x:Class="VtNetCore.Avalonia.App.App"
             xmlns="https://github.com/avaloniaui"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:local="clr-namespace:VtNetCore.Avalonia.App">
	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>

	<Application.Styles>
		<StyleInclude Source="avares://Avalonia.Themes.Default/DefaultTheme.xaml"/>
		<StyleInclude Source="avares://Avalonia.Themes.Fluent/Accents/BaseLight.xaml"/>
		<StyleInclude Source="avares://VtNetCore.Avalonia/VirtualTerminalControl.axaml"/>
	</Application.Styles>
</Application>